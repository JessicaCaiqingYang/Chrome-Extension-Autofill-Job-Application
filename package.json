{
  "name": "job-application-autofill",
  "version": "1.0.0",
  "description": "Chrome extension for automatically filling job application forms",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/build-extension.js",
    "build:complete": "node build-complete.cjs",
    "build:verify": "npm run build && node tests/scripts/test-extension.js && node tests/scripts/verify-extension-functionality.js",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "mammoth": "^1.10.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}